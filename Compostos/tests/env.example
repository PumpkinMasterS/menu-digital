# Configurações de Teste para o Sistema Compostos

# URL da API (ajuste conforme necessário)
API_BASE_URL=http://localhost:5000/api

# Configurações de Teste (opcional - valores padrão serão usados se não definidos)
TEST_USER_NAME=Test User
TEST_USER_EMAIL=test@example.com
TEST_USER_PASSWORD=Test@123456
TEST_USER_PHONE=+1234567890

# Configurações de Robô (opcional)
TEST_ROBOT_NAME=Test Robot
TEST_ROBOT_DESCRIPTION=Robot for testing
TEST_ROBOT_MIN_INVESTMENT=100
TEST_ROBOT_MAX_INVESTMENT=10000
TEST_ROBOT_DAILY_PROFIT=2.5
TEST_ROBOT_DURATION=30
TEST_ROBOT_RISK_LEVEL=Medium

# Configurações de Investimento (opcional)
TEST_INVESTMENT_AMOUNT=500
TEST_INVESTMENT_CURRENCY=USDT
TEST_INVESTMENT_TX_HASH=0x1234567890123456789012345678901234567890

# Configurações de Blockchain (opcional)
USE_BSC_TESTNET=true
COMPANY_BSC_ADDRESS=0x1234567890123456789012345678901234567890

# Configurações de Logging
LOG_LEVEL=INFO

